{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        },
        {
            "src": ".next/**",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        },
        {
            "src": "api/**",
            "use": "@vercel/node"
        },
        {
            "src": "pages/api/**",
            "use": "@vercel/node"
        },
        {
            "src": "pages/**/*.{js,ts,jsx,tsx}",
            "use": "@vercel/node"
        }
    ]
}